make_lm_formula = function(x,y, power){
X = rep(0,power)
if (power>1){
for (i in 2:power){
X[i] = paste0(x,'^',i)
}
return(paste0(y, '~', paste0(X, collapse = '+')))
}else{
return(paste0(y, '~', paste0(x, collapse = '+')))
}
}
